

//CPS
fn r_yes(x : i32, f : fn(i32) -> i32) -> i32 {
    if x == 0 {
        f(9)
    }
    else {f(13)}
}

extern fn r(x: i32) -> i32 {
    r_yes(x,|y| y + 7)
}

extern fn r_no(x : i32) -> i32 {

    let v = if x == 0 {
        9
    }
    else {13};
    v + 7
}

fn pow(@(?x) x: int, n:int) -> i32{
    if n == 0 {1} else {
        x * pow(x,n - 1)
    }
}

extern fn get_32(x : int) -> i32 {
    ife()

}

fn ife() -> i32 {
    if 2==3 {4}
    else if 4==3 {3}
    else if 2 == 2 {0}
    else {12}
}